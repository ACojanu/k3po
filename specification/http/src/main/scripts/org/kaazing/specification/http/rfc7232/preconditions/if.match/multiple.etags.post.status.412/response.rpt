property entityTag "r2d2xxxx"
accept http://localhost:8000/resource
accepted
connected

read method "GET"
read version "HTTP/1.1"
read closed

write status "200" "OK"
write header "ETag" ${entityTag}
write header content-length
write flush
write close

accepted
connected

read method "POST"
read version "HTTP/1.1"
read header "If-Match" "\"invalidETag1\"", "\"invalidETag2\"", "\"invalidETag3\""
read closed

# None of the etags specified using If-Match header match the entity-tag of the
# selected resource. Since the condition evaluates to false, the requested method
# must not be performed. Instead, the response must include
# 412(Precondition Failed) status code.
write status "412" "Precondition Failed"
write flush
write close
