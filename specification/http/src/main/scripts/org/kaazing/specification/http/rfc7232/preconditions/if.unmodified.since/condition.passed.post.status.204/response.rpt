property lastModified "Thu, 1 Jan 2015 01:23:45 GMT"
accept http://localhost:8000/resource
accepted
connected

read method "GET"
read version "HTTP/1.1"
read closed

write status "200" "OK"
write header "Last-Modified" ${lastModified}
write header content-length
write flush
write close

accepted
connected

read method "POST"
read version "HTTP/1.1"
read header "If-Unmodified-Since" ${lastModified}
read closed

# The resource was NOT modified since the date specified as part of
# If-Unmodified-Since header. This means that the condition is true. So,
# continue to perform the requested method and the response will include
# 204(No Content) success status code.
write status "204" "No Content"
write flush
write close
