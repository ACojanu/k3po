accept http://localhost:8000/resource
accepted
connected

read method "GET"
read header "Authorization" "Basic" ${http:loginBase64Encoder("baduser:badpass")}

write status "401" "Unauthorized"
write header "WWW-Authenticate" "Basic realm=\"Example Realm\""
write flush

accepted
connected

read method "GET"

write status "407" "Proxy Authentication Required"
write header "Proxy-Authentication" "Basic realm=\"Example Realm\""
write flush

accepted
connected

read method "GET"
read header "Proxy-Authorization" "Basic" ${http:loginBase64Encoder("joe:welcome")}

write status "200" "OK"
write flush

