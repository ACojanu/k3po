connect http://localhost:8000/index.html
connected

write method "GET"
write version "HTTP/1.1"
write header host
write header content-length
write flush

read status "200" /.*/
read header "ETag" /(?<entityTag>.+)/
read notify CACHE_CONTROL

connect await CACHE_CONTROL
connect http://localhost:8000/index.html
connected

write method "GET"
write version "HTTP/1.1"
write header host
write header "If-None-Match" ${entityTag}
write header "Cache-Control" "no-cache"
write header content-length
write flush

read status "304" /.*/
read header "ETag" /.+/
read header "Cache-Control" /.+/
read closed