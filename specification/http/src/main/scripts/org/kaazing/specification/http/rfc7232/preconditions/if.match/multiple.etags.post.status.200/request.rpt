connect http://localhost:8000/index.html
connected

write method "GET"
write version "HTTP/1.1"
write header content-length
write flush

read status "200" "OK"
read header "ETag" /(?<etag>.+)/
read notify READ_ETAG

connect http://localhost:8000/index.html
connected

write await READ_ETAG
write method "POST"
write version "HTTP/1.1"
write header host
write header "If-Match" ${etag}, "\"tag1\"", "\"tag2\""
write header content-length
write flush

read status "200" "OK"
