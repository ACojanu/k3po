connect http://localhost:8000/resource
connected

write method "GET"
write version "HTTP/1.1"
write header content-length
write flush

read status "200" "OK"
read header "ETag" /(?<etag>.+)/

connect http://localhost:8000/resource
connected

write method "GET"
write version "HTTP/1.1"
write header "If-Match" ${etag}
write header content-length
write flush

read status "304" "Not Modified"
read header "ETag" "abc123"

