connect http://localhost:8000/index.html
connected

write method "GET"
write version "HTTP/1.1"
write header host
write header content-length
write flush

read status "200" "OK"
read header "Last-Modified" /(?<lastModified>.+)/
read notify LAST_MODIFIED

connect http://localhost:8000/index.html
connected

write await LAST_MODIFIED
write method "HEAD"
write header host
write version "HTTP/1.1"
write header "If-Modified-Since" ${lastModified}
write header content-length
write flush

read status "304" "Not Modified"
read header "ETag" /.+/
read closed
